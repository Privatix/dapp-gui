{
    "name": "dappctrlgui",
    "description": "Privatix Network GUI",
    "version": "1.1.0",
    "main": "build/main.js",
    "devDependencies": {
        "@types/colors": "^1.1.3",
        "@types/lodash.isequal": "^4.5.3",
        "@types/lodash.isstring": "^4.0.4",
        "@types/node": "^9.6.32",
        "@types/node-fetch": "^2.1.4",
        "@types/react": "^16.7.18",
        "@types/react-redux": "^6.0.9",
        "@types/react-router": "5.0.0",
        "@types/semver": "^5.5.0",
        "css-loader": "^2.1.1",
        "electron": "^5.0.3",
        "electron-packager": "^13.0.1",
        "electron-react-devtools": "^0.5.3",
        "mocha": "^5.0.1",
        "shx": "^0.2.2",
        "spectron": "^7.0.0",
        "style-loader": "^0.21.0",
        "ts-loader": "^4.5.0",
        "tsconfig-paths-webpack-plugin": "^3.0.3",
        "tslint": "^5.9.1",
        "tslint-loader": "^3.5.3",
        "typescript": "^2.9.2",
        "webpack": "^4.20.2",
        "webpack-cli": "^3.1.1",
        "xunit-file": "^1.0.0"
    },
    "scripts": {
        "start": "./node_modules/.bin/electron .",
        "pretest": "",
        "test": "mocha -R xunit-file tests/spec.js",
        "posttest": "",
        "update_versions": "node ./update_versions.js",
        "prebuild": "shx rm -rf ./build && shx mkdir ./build && node ./build_tools/pre_build.js",
        "build": "tslint -c ./configs/tslint.json -p ./configs/tsconfig.json && webpack --mode development --config configs/webpack.config.js",
        "postbuild": "shx cp -R assets/* ./build/ && node ./build_tools/build.js",
        "package-mac-helper": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --icon=mac.icns --out=release-builds \"dapp-gui\"",
        "package-win-helper": "electron-packager . --overwrite  --platform=win32 --arch=x64 --prune=true --icon=win.ico --out=release-builds --version-string.CompanyName=Privatix --version-string.FileDescription=Privatix --version-string.ProductName=\"Privatix Network\" \"dapp-gui\"",
        "package-linux-helper": "electron-packager . --overwrite --platform=linux --arch=x64 --prune=true --out=release-builds \"dapp-gui\"",
        "package-mac": "cd ./build && npm install && npm run package-mac-helper && shx mv ./release-builds ../",
        "package-win": "cd ./build && npm install && npm run package-win-helper && shx mv ./release-builds ../",
        "package-linux": "cd ./build && npm install && npm run package-linux-helper && shx mv ./release-builds ../",
        "merge_dic": "node ./build_tools/i18nDiff.js"
    },
    "dependencies": {
        "bootstrap": "^4.1.0",
        "btoa": "^1.2.1",
        "bugsnag-js": "^4.7.2",
        "colors": "^1.3.2",
        "cors": "^2.8.4",
        "dateformat": "^3.0.3",
        "electron": "^5.0.3",
        "electron-log": "^3.0.6",
        "fs-extra": "^7.0.1",
        "history": "^4.7.2",
        "i18next": "^11.9.0",
        "jquery": "^3.4.1",
        "lodash.isequal": "^4.5.0",
        "lodash.isstring": "^4.0.1",
        "moment": "^2.22.2",
        "node-fetch": "^2.3.0",
        "node-loader": "^0.6.0",
        "popper.js": "^1.14.3",
        "qrcode": "^1.3.3",
        "rc-slider": "8.6.13",
        "react": "^16.7.0",
        "react-awesome-modal-perfected": "^1.1.1",
        "react-copy-to-clipboard": "^5.0.1",
        "react-countdown-now": "^1.3.0",
        "react-datepicker": "^1.6.0",
        "react-dom": "^16.5.2",
        "react-i18next": "^7.13.0",
        "react-js-pagination": "^3.0.2",
        "react-redux": "^5.0.7",
        "react-router": "5.0.0",
        "react-router-dom": "5.0.0",
        "react-select": "^1.2.1",
        "react-sortable-table-vilan": "^1.0.0",
        "react-sweetalert-vilan": "^1.1.0",
        "react-table": "^6.8.5",
        "react-toggle": "^4.0.2",
        "react-tooltip": "^3.8.4",
        "redux": "^4.0.0",
        "redux-thunk": "^2.3.0",
        "semver": "^5.6.0",
        "sudo-prompt": "^9.0.0",
        "uuid": "^3.2.1"
    },
    "license": "GPL-3.0-only",
    "repository": {
        "type": "git",
        "url": "https://github.com/Privatix/dapp-gui/"
    }
}