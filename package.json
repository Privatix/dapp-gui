{
    "name": "dappctrlgui",
    "description": "Privatix Network GUI",
    "version": "0.15.1",
    "main": "build/main.js",
    "devDependencies": {
        "@types/colors": "^1.1.3",
        "@types/lodash.isequal": "^4.5.3",
        "@types/lodash.isstring": "^4.0.4",
        "@types/node": "^9.6.32",
        "@types/react": "^16.4.14",
        "@types/react-redux": "^6.0.9",
        "css-loader": "^0.28.11",
        "electron": "^1.8.8",
        "electron-packager": "^8.1.0",
        "electron-react-devtools": "^0.5.3",
        "mocha": "^5.0.1",
        "shx": "^0.2.2",
        "spectron": "^3.8.0",
        "style-loader": "^0.21.0",
        "ts-loader": "^4.5.0",
        "tsconfig-paths-webpack-plugin": "^3.0.3",
        "tslint": "^5.9.1",
        "tslint-loader": "^3.5.3",
        "typescript": "^2.9.2",
        "typings-for-css-modules-loader": "^1.7.0",
        "webpack": "^4.20.2",
        "webpack-cli": "^3.1.1",
        "xunit-file": "^1.0.0"
    },
    "scripts": {
        "start": "./node_modules/.bin/electron .",
        "pretest": "",
        "test": "mocha -R xunit-file tests/spec.js",
        "posttest": "",
        "update_versions": "node ./update_versions.js",
        "build": "shx rm -rf ./build && shx mkdir ./build && tslint -c ./configs/tslint.json -p ./configs/tsconfig.json && webpack --mode development --config configs/webpack.config.js && shx cp -R assets/* ./build/ && node ./build.js",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
        "package-win": "electron-packager . --overwrite  --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=Privatix --version-string.FileDescription=Privatix --version-string.ProductName=\"Privatix Network\"",
        "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --prune=true --out=release-builds"
    },
    "dependencies": {
        "async-reactor": "^1.2.2",
        "bootstrap": "^4.1.0",
        "btoa": "^1.2.1",
        "bugsnag-js": "^4.7.2",
        "colors": "^1.3.2",
        "cors": "^2.8.4",
        "dateformat": "^3.0.3",
        "electron-fetch": "1.1.0",
        "history": "^4.7.2",
        "i18next": "^11.9.0",
        "jquery": "^3.3.1",
        "lodash.isequal": "^4.5.0",
        "lodash.isstring": "^4.0.1",
        "moment": "^2.22.2",
        "node-loader": "^0.6.0",
        "popper.js": "^1.14.3",
        "rc-slider": "^8.6.3",
