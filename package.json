        "react-copy-to-clipboard": "^5.0.1",
        "react-countdown-now": "^1.3.0",
        "react-datepicker": "^1.6.0",
        "react-dom": "^16.5.2",
        "react-i18next": "^7.13.0",
        "react-js-pagination": "^3.0.2",
        "react-redux": "^5.0.7",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-select": "^1.2.1",
        "react-sortable-table-vilan": "^1.0.0",
        "react-sweetalert-vilan": "^1.0.0",
        "react-table": "^6.8.5",
        "react-toggle": "^4.0.2",
        "react-tooltip": "^3.8.4",
        "redux": "^4.0.0",
        "redux-thunk": "^2.3.0",
        "uuid": "^3.2.1"
